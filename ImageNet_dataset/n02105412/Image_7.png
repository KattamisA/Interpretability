			$(document).ready(function() {
				
				var navbarOffset = $('header').next().offset().top - 20;
				
				$('#navbar').parent().parent().affix({
					offset: navbarOffset
				});
				
				$('#navbar a').on('click', function(event) {
					
					// get href of clicked anchor
					var id = $(this).attr('href');
					
					if(!$(id).length) {
						// element does not exist, so must not be an in-page link, navigate to link
					} else {
						// prevent default jump to url
						event.preventDefault();
						// Jump to anchor
						$('html, body').animate({scrollTop: ($(id).offset().top - 10)},'slow');
					}
				});
				
				// Initialise Scrollspy
				$(window).scrollspy({
					wrap: $('#wrap')[0]
				});
				
			});
